{
    "name": "michelmelo/laravel-console-create-view",
    "description": "Create Laravel views (blade template) using 'php artisan' command-line interface",
    "type": "library",
    "keywords": [
        "laravel",
        "views",
        "blade",
        "template",
        "generator",
        "php",
        "artisan",
        "command"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Michel Melo",
            "email": "eu@michelmelo.pt"
        }
    ],
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "MichelMelo\\ConsoleView\\": "src/"
        }
    },
    "extra": {
        "laravel":{
            "providers":[
                "MichelMelo\\ConsoleView\\ConsoleViewServiceProvider"
            ]
        }
    }
}